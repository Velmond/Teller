@model IEnumerable<Teller.Web.ViewModels.Story.UserFeedStory>

<div class="row">
    @if(Model.Count() == 0)
    {
        <h3 class="lead text-center">
            No stories were found
        </h3>
    }
    else
    {
        foreach(var story in Model)
        {
            <div class="col-sm-12">
                @Html.DisplayFor(m => story)
            </div>
        }
    }
</div>